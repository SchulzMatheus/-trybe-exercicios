let numbers = [5, 9, 3, 19, 70, 8, 100, 2, 35, 27];
let x;

for (let i = 0; i<numbers.length; i++){
    for(let i2 = 0; i2<(numbers.length-i-1); i2++){
        if(numbers[i2]<numbers[i2+1]){
            x = numbers[i2]
            numbers[i2] = numbers[i2+1]
            numbers[i2+1] = x
        }
    }
}
console.log(numbers)